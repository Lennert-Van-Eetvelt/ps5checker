{
	"name":"Ps5 checker",
	"version":"1.0",
	"manifest_version":2,

	"description": "Hi!",
	"icons":{"128":"icon.png"},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-3.5.1.min.js", "content.js"],
			"run_at": "document_start"
		}
		],

	"browser_action":{
	"default_title": "",
	"default_popup": "popup.html"},
	
	"permissions": [
		"<all_urls>","storage"
		]
}